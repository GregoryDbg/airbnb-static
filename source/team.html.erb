<h1 class="text-center">Our team</h1>

  <div class="container text-center">
    <div class="row">
      <% data.team_members.each do |member| %>
        <div class="col-xs-6">
          <h2><%= member[:name]%></h2>
          <img class= "avatar avatar-large avatar-bordered" src="<%= member[:image]%>" alt="picture">
        </div>
      <% end %>
    </div>
    <h3 class="padded">
      <a <i class="fa fa-envelope" href="mailto:contact@airbnb.com">contact@airbnb.com</a>
    </h3>
  </div>




